# Demo Mode Configuration
# Use this to run the frontend without Supabase setup
# Copy to .env.local to enable demo mode

# Demo Mode (bypasses real authentication)
VITE_DEMO_MODE=true
VITE_TEST_MODE=true

# Fake Supabase URLs (won't actually connect)
NEXT_PUBLIC_SUPABASE_URL=https://demo.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=demo-key-for-ui-only
SUPABASE_SERVICE_ROLE_KEY=demo-service-key

# Application Configuration
NEXT_PUBLIC_APP_ENV=demo
NEXT_PUBLIC_APP_NAME=FitTracker
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=false
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# Development Tools
NEXT_PUBLIC_SHOW_DEBUG_INFO=true
NEXT_PUBLIC_ENABLE_REACT_QUERY_DEVTOOLS=true